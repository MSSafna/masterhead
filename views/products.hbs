

<body>
    <div class="page-wrapper">


        <main class="main mt-3">



            <div class="page-content">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-9">
                            <div class="toolbox">



                            </div><!-- End .toolbox -->

                            <div class="products mb-3">

                                <div class="product product-list ml-5">
                                   
                                    {{#each datas}}
                                    {{#if this.status}}
                                    <div class="row mt-3">
                                        <div class="col-6 col-lg-3 ">
                                            <figure class="product-media">
                                                <span class="product-label label-new">New</span>
                                                {{!-- <a href="/zoomproduct/{{this._id}}"> --}}
                                                   <a href="/zoomproducttesting/{{this._id}}"> 
                                                    <img src="/images/product/{{this.img.[0]}}" alt="Product image"
                                                        class="product-image">
                                                </a>
                                            </figure><!-- End .product-media -->
                                            
 
                                        </div><!-- End .col-sm-6 col-lg-3 -->

                                        <div class="col-6 col-lg-3 order-lg-last ">
                                            <div class="product-list-action ">
                                            {{#if this.brand.offerDisplay}}

                                            <div class="product-price">
                                                <strong>
                                                Rs:&nbsp;{{this.price}}&nbsp;&nbsp;
                                                </strong>
                                                <span> <del> {{this.originalPrice}}</del></span>
                                            </div>
                                            <span style="color: green;"><strong> &nbsp;{{this.brand.offerPercentage}}%
                                                    off</strong></span>
                                            
                                            {{else}}
                                            
                                                <div class="product-price"><strong>
                                                   RS :&nbsp; {{this.price}}</strong>
                                                </div><!-- End .product-price -->
                                              {{/if}}

                                               <p class="product-price">Brand :
                                                    {{this.brand.brand}}
                                                </p>
                                                <div class="ratings-container">
                                                    <div class="ratings">
                                                        <div class="ratings-val" style="width: 20%;"></div>
                                                        <!-- End .ratings-val -->
                                                    </div><!-- End .ratings -->
                                                    <span class="ratings-text">( 2 Reviews )</span>
                                                </div><!-- End .rating-container -->

                                                <!-- End .product-action -->
                                                {{#if this.stock}}
                                                {{#if this.cartPresent}}
                                                <a href="/getcart/{{this._id}}"  type="btn" class="btn btn-outline-danger btn-lg btn-block" >go to cart</a>
                                              
                                                   {{else}}
                                                   <button type="button" class="btn btn-outline-danger btn-lg btn-block"
                                                onclick="addToCart('{{this._id}}')">Add to cart</button>
                                                  
                                                        {{/if}}    
                                                {{else}}
                                                <a href="" class="btn-product btn-cart" inactive><span>out of stock
                                                    </span></a>
                                                {{/if}}
                                                 {{#if wishList}}
                                                     <a href=""  style="float: right;margin-top:5px;"><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-heart-fill" viewBox="0 0 16 16">
                                                <a href=""> <path fill-rule="evenodd" d="M8 1.314C12.438-3.248 23.534 4.735 8 15-7.534 4.736 3.562-3.248 8 1.314z"/>
                                            </svg> Added to wishlist</a> 
                                                
                                                {{else}}
                                                <a href="/wishlist/{{this._id}}"  style="float: right;margin-top:5px;" ><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-heart-fill" viewBox="0 0 16 16">
                                              <path d="m8 2.748-.717-.737C5.6.281 2.514.878 1.4 3.053c-.523 1.023-.641 2.5.314 4.385.92 1.815 2.834 3.989 6.286 6.357 3.452-2.368 5.365-4.542 6.286-6.357.955-1.886.838-3.362.314-4.385C13.486.878 10.4.28 8.717 2.01L8 2.748zM8 15C-7.333 4.868 3.279-3.04 7.824 1.143c.06.055.119.112.176.171a3.12 3.12 0 0 1 .176-.17C12.72-3.042 23.333 4.867 8 15z"/>
                                               </svg> Add to wishlist</a>
                                             {{/if}}
                                               
                                            </div><!-- End .product-list-action -->
                                        </div><!-- End .col-sm-6 col-lg-3 -->

                                        <div class="col-lg-6 ">
                                            <div class="product-body product-action-inner ml-3">
                                                {{!-- <a href="/wishlist/{{this._id}}" class="btn-product btn-wishlist "
                                                    title="Add to wishlist"><span>add to wishlist </span></a> --}}

                                                <h3 class="product-title"><a href="/products?page=1"><strong>{{this.name}}</strong></a></h3>
                                                <!-- End .product-title -->

                                                <div class="product-content">
                                                    <p>{{this.discription}} </p>
                                                </div><!-- End .product-content -->

                                                <div class="product-nav product-nav-thumbs">
                                                    {{#each img}}
                                                    <a href="#" class="active">
                                                        <img src="/images/product/{{this}}" 
                                                            alt="product desc">
                                                    </a>
                                                   {{/each}}
                                                </div><!-- End .product-nav -->
                                            </div><!-- End .product-body -->
                                        </div><!-- End .col-lg-6 -->
                                    </div><!-- End .row -->
                                    {{/if}}
                                    {{/each}}
                                </div><!-- End .product -->


                            </div><!-- End .products -->

                            <nav aria-label="Page navigation">
                                <ul class="pagination">
                                    
                                        {{#productsPrev '1' currentPage}}
                                       
                                        
                                        <li class="page-item disabled">
                                        <a class="page-link page-link-prev "  href="" aria-label="Previous" tabindex="-1"
                                            aria-disabled="true">
                                            <span aria-hidden="true"><i class="icon-long-arrow-left"></i></span>Prev
                                        </a>
                                        {{else}}
                                    
                                        <a class="page-link page-link-prev "  href="/products/?page={{../previous.page}}" aria-label="Previous" tabindex="-1"
                                            aria-disabled="true">
                                            <span aria-hidden="true"><i class="icon-long-arrow-left"></i></span>Prev
                                        </a>
                                        {{/productsPrev}}
                                    </li>
                                    {{#each pages}}
                                    <li class="page-item active" aria-current="page"><a class="page-link" href="/products/?page={{this}}" >{{this}}</a>
                                    </li>
                                    {{/each}}

                                    {{#productsNext  currentPage pages.length}}
                                    <li class="page-item disabled">
                                        <a class="page-link page-link-next" href="#" aria-label="Next">
                                            Next <span aria-hidden="true"><i class="icon-long-arrow-right"></i></span>
                                        </a>
                                    </li>
                                    {{else}}
                                    <li class="page-item">
                                        <a class="page-link page-link-next" href="/products/?page={{../next.page}}" aria-label="Next">
                                            Next <span aria-hidden="true"><i class="icon-long-arrow-right"></i></span>
                                        </a>
                                    </li>
                                    {{/productsNext}}
                                </ul>
                            </nav>
                        </div><!-- End .col-lg-9 -->
                        <aside class="col-lg-3 order-lg-first">
                            <div class="sidebar sidebar-shop">
                                <div class="widget widget-clean">
                                    <label>Filters:</label>
                                    <a href="#" class="sidebar-filter-clear">Clean All</a>
                                </div><!-- End .widget widget-clean -->

                                <div class="widget widget-collapsible">
                                    <h3 class="widget-title">
                                        <a data-toggle="collapse" href="#widget-1" role="button" aria-expanded="true"
                                            aria-controls="widget-1">
                                            Category
                                        </a>
                                    </h3><!-- End .widget-title -->

                                    <div class="collapse show" id="widget-1">
                                        <div class="widget-body">
                                            <form action="/brandFilter" method="post">
                                            <div class="filter-items filter-items-count">
                                                   {{#each brands}} 
                                                <div class="filter-item">
                                                   
                                                    <div class="custom-checkbox">
                                                        <input type="checkbox" value="{{this._id}}" name="brandFilter">
                                                        <label >{{this.brand}}</label>
                                                    </div><!-- End .custom-checkbox -->    
                                                </div><!-- End .filter-item -->
                                                      {{/each}} 
                                                     <button class=" btn btn-outline-primary mt-3" type="submit">Apply filter</button>
                                            </div><!-- End .filter-items -->
                                        </form>
                                        </div><!-- End .widget-body -->
                                    </div><!-- End .collapse -->
                                </div><!-- End .widget -->
                            </div><!-- End .sidebar sidebar-shop -->
                        </aside><!-- End .col-lg-3 -->
                    </div><!-- End .row -->
                </div><!-- End .container -->
            </div><!-- End .page-content -->
        </main><!-- End .main -->

        
    </div><!-- End .page-wrapper -->
    <button id="scroll-top" title="Back to Top"><i class="icon-arrow-up"></i></button>

    <!-- Mobile Menu -->
    <div class="mobile-menu-overlay"></div><!-- End .mobil-menu-overlay -->

    

    <!-- Sign in / Register Modal -->
    
<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
   
</body>
</html>