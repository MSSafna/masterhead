            };
            margins = {
                top: 150,
                bottom: 60,
                left: 40,
                right: 40,
                width: 600
            };
            var y = 20;
            doc.setLineWidth(2);
            doc.text(200, y = y + 30, "Weekly Sales Report");
            doc.autoTable({
                html: '#dailyReport',
                startY: 70,
                theme: 'grid',
