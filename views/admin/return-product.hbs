                                        <td class="tm-product-name">{{this.orderId}}</td>
                                        <td class="tm-product-name">{{this.productId}}</td>
                                        <td class="tm-product-name">{{this.paymentMethod}}</td>
                                        <td class="tm-product-name">{{this.productStatus}}</td>
                                        <td>
                                            
                                            <a href="/admin/return-product-details/?orderId={{this.orderId}}&productId={{this.productId}}"
                                                class="btn btn-primary " >More</a>
                                                
                                        </td>

                                    </tr>

                                    {{/each}}


                                </tbody>

                            </table>




                            <!-- End Table with hoverable rows -->

                        </div>
                    </div>


                </div>

                
                <div class="tab-pane fade" id="bordered-justified-contact" role="tabpanel"
